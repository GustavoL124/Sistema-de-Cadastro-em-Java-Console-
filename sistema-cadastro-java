public class Usuario {
    private String nome;
    private int idade;
    private String email;

    public Usuario(String nome, int idade, String email) {
        this.nome = nome;
        this.idade = idade;
        this.email = email;
    }

    public String getNome() {
        return nome;
    }

    public String toString() {
        return "Nome: " + nome + " | Idade: " + idade + " | Email: " + email;
    }
}
